      <Label layoutX="83.0" layoutY="128.0" text="Username">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <TextField fx:id="username" layoutX="83.0" layoutY="159.0" prefHeight="25.0" prefWidth="249.0" />
      <PasswordField fx:id="password" layoutX="83.0" layoutY="230.0" prefHeight="25.0" prefWidth="249.0" />
      <Label layoutX="85.0" layoutY="200.0" text="Password">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <Button fx:id="login" layoutX="182.0" layoutY="298.0" mnemonicParsing="false" onAction="#login" text="Login" />
   </children>
